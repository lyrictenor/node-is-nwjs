{
  "name": "is-nwjs",
  "description": "Check if your code is running on NW.js.",
  "version": "0.0.0",
  "author": {
    "name": "sanemat",
    "email": "o.gata.ken@gmail.com",
    "url": "sane.jp"
  },
  "dependencies": {},
  "devDependencies": {
    "conventional-changelog": "^0.1.0-beta",
    "david": "^6.1.6",
    "eclint": "^0.2.6",
    "eslint": "^1.0.0-rc",
    "fixpack": "^2.2.0",
    "globstar": "^1.0.0",
    "touch": "^1.0.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "files": [
    "index.js",
    "is-nodejs.js"
  ],
  "keywords": [
    "NW.js", "node-webkit"
  ],
  "license": "MIT",
  "repository": "pandawing/node-is-nwjs",
  "scripts": {
    "changelog": "touch changelog.md && conventional-changelog -i changelog.md --overwrite --preset angular",
    "david": "david update",
    "eclint": "globstar --ignore \"node_modules/**\" -- eclint check \"*.{js,yml,md,json}\"",
    "eslint": "eslint .",
    "fixpack": "fixpack",
    "git:tag": "git tag v${npm_package_version}",
    "lint": "npm run eclint && npm run eslint",
    "verify": "npm run lint"
  }
}
